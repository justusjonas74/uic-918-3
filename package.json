{
  "name": "uic-918-3",
  "version": "0.2.0",
  "description": "Package for decoding and parsing barcodes according to UIC-918.3 specification, which are used commonly on public transport online tickets.",
  "main": "index.js",
  "scripts": {
    "standard-fix": "standard --fix",
    "test": "standard && NODE_ENV=test istanbul cover ./node_modules/mocha/bin/_mocha --report lcov -- -R spec",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "author": "Francis Doege",
  "license": "MIT",
  "dependencies": {
    "enum": "^2.5.0",
    "iconv-lite": "^0.4.19",
    "zebra-crossing": "^0.1.1"
  },
  "directories": {
    "lib": "lib"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "chai-properties": "^1.3.0",
    "chai-things": "^0.2.0",
    "coveralls": "^3.0.0",
    "dirty-chai": "^2.0.1",
    "istanbul": "^0.4.5",
    "mocha": "^5.0.0",
    "randomstring": "^1.1.5",
    "standard": "^10.0.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/justusjonas74/uic-918-3.git"
  },
  "standard": {
    "env": [
      "mocha"
    ]
  },
  "keywords": [
    "uic-918-3",
    "online-ticket",
    "deutsche-bahn",
    "barcode",
    "aztec"
  ],
  "bugs": {
    "url": "https://github.com/justusjonas74/uic-918-3/issues"
  },
  "homepage": "https://github.com/justusjonas74/uic-918-3#readme"
}
